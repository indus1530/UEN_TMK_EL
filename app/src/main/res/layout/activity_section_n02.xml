                                android:id="@+id/qtxt_can20"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/q_can20"
                                android:text="@string/can20" />

                            <TextView
                                android:id="@+id/q_can20"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_can20"
                                android:layout_alignBottom="@id/qtxt_can20"
                                android:text="@string/Q_can20" />
                        </RelativeLayout>

                        <RadioGroup
                            android:id="@+id/can20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/can2001"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/can2001" />

                            <RadioButton
                                android:id="@+id/can2002"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/can2002" />

                        </RadioGroup>

                    </LinearLayout>


                            android:layout_marginBottom="12dp"
                            android:hint="@string/can21"
                            android:inputType="number"
                            android:maxLength="2"
                            app:mask="##"
                            app:maxValue="30"
                            app:minValue="10"
                            app:type="range" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>


            </LinearLayout>


            <!--EndButton LinearLayout-->
            <LinearLayout
                android:layout_width="match_parent"
